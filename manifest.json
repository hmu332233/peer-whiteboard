{
  "name": "peer whiteboard",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "",
  "icons": {
    "128": "assets/images/icon.png"
  },
  "permissions": [],
  "sidebar_action": {
    "default_page": "dist/index.html",
    "default_icon": {
        "16": "assets/images/icon.png"
    },
    "default_title": "Peer Whiteboard",
    "use_navigation_bar": false,
    "mobile_user_agent": false
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["dist/contentScript.js"]
    }
  ]
}
